{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "missing",
          "value": "file"
        }
      ],
      "statusCode": 200
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
